<!DOCTYPE html>
<html lang="en"><!-- Basic -->
<head>
	<meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">   
   
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
 
     <!-- Site Metas -->
    <title>Kakefood Bakery </title>  
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Site Icons -->
    <link rel="shortcut icon" href="/public/resources/views/favicon.ico" type="/public/resources/views//x-icon">
    <link rel="apple-touch-icon" href="/public/resources/views/apple-touch-icon.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/public/resources/views/css/bootstrap.min.css">    
	<!-- Site CSS -->
    <link rel="stylesheet" href="/public/resources/views/css/style.css">    
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="/public/resources/views/css/responsive.css">
	<!-- Custom CSS -->
    <link rel="stylesheet" href="/public/resources/views/css/custom.css">

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>
	<!-- Start header -->
	{{>header}}
	<!-- End header -->
	
	<!-- Start All Pages -->
	<div class="all-page-title page-breadcrumb">
		<div class="container text-center">
			<div class="row">
				<div class="col-lg-12">
					<h1>Special Menu</h1>
				</div>
			</div>
		</div>
	</div>
	<!-- End All Pages -->
	
	<!-- Start Menu -->
	<div class="menu-box">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="heading-title text-center">
						<h2>Menu</h2>
						<p>Make your right choosen ;D</p>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<div class="special-menu text-center">
						<div class="button-group filter-button-group">
							<button class="active" data-filter="*">All</button>
							{{#each cate}}
							<button data-filter=".{{this.Name_category}}">{{this.Name_category}}</button>
							{{/each}}
						</div>
					</div>
				</div>
			</div>
				{{!-- <form action="/giohang/huhu/4" method="post">
					<button type="submit">Buy now</button>
				</form> --}}
			<div class="row special-list">
				{{#each prod}}
				<div class="col-lg-4 col-md-6 special-grid {{this.Name_category}}">
					<div class="gallery-single fix">
						<img src="{{this.Link_image}}" class="img-fluid" alt="/public/resources/views/">
						<form action="/giohang/huhu/{{this.Id_product}}" method="post">
							<div class="why-text">
								<h4>{{this.Name_product}}</h4>
								<p>{{this.Describe}}</p>
								<h5>${{this.Price}}</h5>
								<button type="submit">Buy now</button>
							</div>
						</form>
					</div>
				</div>
				{{/each}}
				
				
			</div>
		</div>
	</div>
	<!-- End Menu -->
	
	<!-- Start QT -->
	<div class="qt-box qt-background">
		<div class="container">
			<div class="row">
				<div class="col-md-8 ml-auto mr-auto text-left">
					<p class="lead ">
						" If you're not the one cooking, stay out of the way and compliment the chef. "
					</p>
					<span class="lead">Michael Strahan</span>
				</div>
			</div>
		</div>
	</div>
	<!-- End QT -->
	
	
	
	<!-- Start Contact info -->
	{{>contact}}
	<!-- End Contact info -->
	
	<!-- Start Footer -->
	{{>footer}}
	<!-- End Footer -->
	
	<a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>

	<!-- ALL JS FILES -->
	<script src="/public/resources/views/js/jquery-3.2.1.min.js"></script>
	<script src="/public/resources/views/js/popper.min.js"></script>
	<script src="/public/resources/views/js/bootstrap.min.js"></script>
    <!-- ALL PLUGINS -->
	<script src="/public/resources/views/js/jquery.superslides.min.js"></script>
	<script src="/public/resources/views/js/images-loded.min.js"></script>
	<script src="/public/resources/views/js/isotope.min.js"></script>
	<script src="/public/resources/views/js/baguetteBox.min.js"></script>
	<script src="/public/resources/views/js/form-validator.min.js"></script>
    <script src="/public/resources/views/js/contact-form-script.js"></script>
    <script src="/public/resources/views/js/custom.js"></script>
</body>
</html>